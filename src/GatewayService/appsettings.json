{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "encheresRead": {
        "ClusterId": "encheres",
        "Match": {
          "Path": "/encheres/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "api/encheres/{**catch-all}"
          }
        ]
      },
      "encheresWrite": {
        "ClusterId": "encheres",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/encheres/{**catch-all}",
          "Methods": [
            "POST",
            "PUT",
            "DELETE"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "api/encheres/{**catch-all}"
          }
        ]
      },
      "recherche": {
        "ClusterId": "recherche",
        "Match": {
          "Path": "/recherche/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "api/recherche/{**catch-all}"
          }
        ]
      }
    }
  }
}